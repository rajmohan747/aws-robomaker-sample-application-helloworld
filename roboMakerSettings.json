{
  "runConfigurations": [
    {
      "id": "HelloWorld_Cfg01",
      "name": "HelloWorld Robot",
      "type": "colcon build",
      "cfg": {
        "workingDir": "/aws-robomaker-sample-application-helloworld/robot_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "HelloWorld_Cfg02",
      "name": "HelloWorld Robot",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "/aws-robomaker-sample-application-helloworld/robot_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "HelloWorld_Cfg03",
      "name": "HelloWorld Simulation",
      "type": "colcon build",
      "cfg": {
        "workingDir": "/aws-robomaker-sample-application-helloworld/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "HelloWorld_Cfg04",
      "name": "HelloWorld Simulation",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "/aws-robomaker-sample-application-helloworld/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "HelloWorld_SimulationJob1",
      "name": "HelloWorld",
      "type": "simulation",
      "cfg": {
        "robotApp": {
          "name": "RoboMakerHelloWorldRobot<capitalised name of ROS distribution, e.g. Kinetic>",
          "sourceBundleFile": "./HelloWorld/robot_ws/bundle/output.tar",
          "s3Bucket": "helloworlddemobucket",
          "architecture": "X86_64",
          "launchConfig": {
            "packageName": "hello_world_robot",
            "launchFile": "rotate.launch",
            "environmentVariables": {
              "TURTLEBOT3_MODEL": "waffle_pi"
            }
          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        },
        "simulationApp": {
          "name": "RoboMakerHelloWorldSimulation<capitalised name of ROS distribution, e.g. Kinetic>",
          "sourceBundleFile": "./HelloWorld/simulation_ws/bundle/output.tar",
          "architecture": "X86_64",
          "s3Bucket": "helloworlddemobucket",
          "launchConfig": {
            "packageName": "hello_world_simulation",
            "launchFile": "empty_world.launch",
            "environmentVariables": {
              "TURTLEBOT3_MODEL": "waffle_pi"
            }
          },
          "simulationSoftwareSuite": {
            "name": "Gazebo",
            "version": "9"
          },
          "renderingEngine": {
            "name": "OGRE",
            "version": "1.x"
          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        },
        "simulation": {
          "maxJobDurationInSeconds": 28800,
          "failureBehavior": "Fail",
          "iamRole": "arn:aws:iam::377900838358:role/aws-service-role/robomaker.amazonaws.com/AWSServiceRoleForRoboMaker",
          "outputLocation": "<bucket name for your job outputs>",
          "tags": {}
        }
      }
    },
    {
      "id": "HelloWorld_wf1",
      "type": "workflow",
      "name": "HelloWorld - Build and Bundle All",
      "runCfgIds": [
        "HelloWorld_Cfg01",
        "HelloWorld_Cfg02",
        "HelloWorld_Cfg03",
        "HelloWorld_Cfg04"
      ]
    }
  ]
}